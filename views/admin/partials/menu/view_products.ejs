<br>
<ol class="breadcrumb mb-4">
    <li class="breadcrumb-item active">Products</li>
</ol>
 <div class="row">
     <div class="col-12">
         <a href="/admin/add-product"><button class="btn btn-primary">Add New Product</button> </a>
         <a href="/admin/update-stock"><button class="btn btn-primary">Update Stock</button> </a>
     </div>
 </div>
<br>
 <table class="table table-bordered" >
    <thead class="thead-dark" >
        <tr>
            <th>Name</th>
            <th>Brand</th>
            <th>Category</th>
            <th>Model</th>
            <th>price</th>
            <th>quandity</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody>
        <% if(products != 'undefined' ){ %>
            <% products.forEach(function(product){ %>
                <tr>
                    <td><%= product.name %></td>
                    <td><%= product.brand %></td>
                    <td><%= product.category %></td>
                    <td><%= product.model %></td>
                    <td><%= product.price %></td>
                    <td><%= product.quantity %></td>
                    <td><img src="/product-images/<%= product._id %>.jpg" alt="product" width="50px"></td>
                </tr>
            <% }); %>
        <% } %>
    </tbody>
</table>